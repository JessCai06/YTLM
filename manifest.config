{
  "manifest_version": 3,
  "name": "YouTube AI Navigator",
  "version": "1.0",
  "description": "Navigate YouTube videos using AI and transcripts.",
  "permissions": ["storage"],
  "host_permissions": ["https://www.youtube.com/*"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}